
// pass by value and reference
func f(a, b&) {
	a += 1;
	b += 1;
	println(a, " ", b);
}

func main() {	

	var in = File("tests/large_file", File.READ);
	var line = in.read_line();
	var l = 0;
	while(line) {
		println(l+=1, line = in.read_line());
	}
	in.close();

	// operator overloading
	var path = Path("C:/dev") / "path/to" / "file.txt";
	println(path);

	var a = 1, b = 1;
	f(a, b);
	println(a, " ", b);
}
