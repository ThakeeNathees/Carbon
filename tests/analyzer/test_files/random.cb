func binary_search(arr, l, r, x) {

    if (r >= l) { 
        var mid = l + (r - l) / 2; 
  
        if (arr[mid] == x)  return mid; 
			
        if (arr[mid] > x) 
            return binary_search(arr, l, mid - 1, x); 
  
        return binary_search(arr, mid + 1, r, x); 
    }
  
    return -1; 
} 

